<h1>All Artists</h1>
<ul>
  <% @artists.each do |artist| %>
  <li>
    <ul>
      <li><%= image_tag(artist.image, :class => 'thumb') %></li>
      <li><%= link_to(artist.name, artist) %></li>
      <li><%= artist.nationality %></li>
      <li><%= artist.dob.strftime '%A, %B %e, %Y' unless artist.dob.nil? %></li>  
      <li><%= artist.period %></li>
    </ul>
  </li>
  <% end %>
</ul>  
